@model VehicleQuote.Models.MyModel

<p>Test VD 123</p>
<script type="text/javascript">
    // create and populate model object
    function createNews() {
        var myModel =
                    {
                        "makeId": $('#Title').val(),
                        "modelId": $('#Content').val()
                    };

        var jsonToPost = JSON.stringify(myModel);

        $.ajax({
            url: '/test123/Create/',
            async: true,
            processData: false,
            data: jsonToPost,
            type: 'POST',
            contentType: 'application/json; charset=utf-8',
            success: function (data) {
                // on success
            },
            error: function (xhr, param) {
                console.log(xhr);
                console.log(param);
            }
        });
    }
</script>

<div>
    <div class="form-group">
        @Html.LabelFor(x => Model.makeId, htmlAttributes: new { @class = "control-label col-md-2" })
        <div class="col-md-5">
            @Html.DropDownListFor(x => Model.makeId, new SelectList(Model.lstMakes, "id", "description"), htmlAttributes: new { @class = "form-control", id = "CarMake" })
            @Html.ValidationMessageFor(x => x.makeId, "", new { @class = "text-danger" })
        </div>
    </div>

    <input type="button" onclick="createNews()" value="Save" />
</div>
